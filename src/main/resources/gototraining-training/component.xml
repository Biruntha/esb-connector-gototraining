<component name="gototraining-training" type="synapse/template">
    <subComponents>
        <component name="createTraining">
            <file>createTraining.xml</file>
            <description>Create a Training in GoToTraining.</description>
        </component>
        <component name="listTrainings">
            <file>listTrainings.xml</file>
            <description>This call retrieves information on all scheduled trainings for a given organizer.</description>
        </component>
        <component name="updateTraining">
            <file>updateTraining.xml</file>
            <description>Request to update a scheduled training.</description>
        </component>
        <component name="getTraining">
            <file>getTraining.xml</file>
            <description>Use the organizer key and training key to retrieve information on a scheduled training.
            </description>
        </component>
    </subComponents>
</component>