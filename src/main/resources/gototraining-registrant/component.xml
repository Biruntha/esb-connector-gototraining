<component name="gototraining-registrant" type="synapse/template">
    <subComponents>
        <component name="addRegistrant">
            <file>addRegistrant.xml</file>
            <description>Register one person, identified by a unique email address, for a training.</description>
        </component>
        <component name="listRegistrants">
            <file>listRegistrants.xml</file>
            <description>Retrieve details on all registrants for a specific training.</description>
        </component>
        <component name="getRegistrant">
            <file>getRegistrant.xml</file>
            <description>Retrieve details for specific registrant in a specific training.</description>
        </component>
    </subComponents>
</component>